{
  "name": "floyd-steinberg-algorithm-js",
  "version": "2.0.0",
  "description": "This is an implementation of Floyd-Steinberg Algorithm.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "type-check": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint:check": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/eslint": "^8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vitest-canvas-mock": "^0.3.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0"
  },
  "keywords": [
    "Floyd-Steinberg",
    "grayscale",
    "reverse pixels"
  ],
  "author": "Jakub Grzywaczewski",
  "license": "MIT",
  "homepage": "https://github.com/jakubgrzywaczewski/floyd-steinberg-algorithm-js"
}
